      <mibObj oid=".1.3.6.1.4.1.193.81.3.4.5.1.2.1.2" instance="xfLIMappingTable" alias="xfifIndex" type="string"/>
      <mibObj oid=".1.3.6.1.4.1.193.81.3.4.5.1.2.1.3" instance="xfLIMappingTable" alias="xfifName" type="string"/>
