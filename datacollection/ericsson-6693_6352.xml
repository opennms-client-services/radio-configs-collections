         <parameter key="29" value="acm4096QAMStd(29)"/>
         <parameter key="30" value="acm4096QAMLight(30)"/>
      </property>
   </group>
   <group name="ericsson_6693_ptp_radio" ifType="all">
      <mibObj oid=".1.3.6.1.4.1.193.81.3.4.3.1.2.1.1" instance="ericsson_ifIndex" alias="xfRFBaseTxFreq" type="string"/>
      <mibObj oid=".1.3.6.1.4.1.193.81.3.4.3.1.2.1.2" instance="ericsson_ifIndex" alias="xfRFBaseRxFreq" type="string"/>
      <mibObj oid=".1.3.6.1.4.1.193.81.3.4.3.1.2.1.3" instance="ericsson_ifIndex" alias="xfRFFreqStep" type="string"/>
